#include<iostream>
#include<deque>
#define endl "\n"
using namespace std;

int main(){
	deque<int> dq;
	//insert at back size
	dq.push_back(1);
	dq.push_back(2);
	//insert at front size
	dq.push_front(3);
	dq.push_front(4);

	for(auto x: dq)
		cout<<x<<" ";
	cout<<endl;

	//delete now
	dq.pop_back();
	dq.pop_front();

	for(auto x: dq)
		cout<<x<<" ";
	cout<<endl;


}
