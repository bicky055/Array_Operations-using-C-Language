#include "bits/stdc++.h"
using namespace std;
//micro functions
#define endl "\n";
#define mod 1000000007
#define ll long long
#define vi vector<int>
#define pii pair<int,int>
#define vii vector<pii>
#define rep(i,a,b) for(int i=a; i<b; i++)
#define ff first
#define ss second
#define setBits(x) builtin_popcount(x)

int main(){
  //declear MaxHeap
  priority_queue<int,vector<int> > pq;
  pq.push(2);
  pq.push(1);
  pq.push(3);
  pq.push(5);
  cout<<pq.top()<<endl;
  pq.pop();
  cout<<pq.top()<<endl;
  cout<<endl;

  //declear MinHeap
  priority_queue<int, vector<int>, greater<int> > pqm;
  pqm.push(2);
  pqm.push(5);
  pqm.push(10);
  pqm.push(3);

  cout<<pqm.top()<<endl;
  pqm.pop();
  cout<<pqm.top()<<endl;

}
